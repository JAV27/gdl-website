<div class="col s12 l4">
    <div class="card hoverable">
        <div class="card-content waves-effect waves-green activator">
            <span class="card-menu card-title green-text text-darken-2"><i class="material-icons">menu</i></span>
            <img class="soccerguy" src="/images/<%= image %>">
            <span class="card-title green-text text-darken-2"><%= title %></span>
        </div>
        <div class="card-reveal">
          <span class="card-title green-text text-darken-2"><%= title %><i class="material-icons right">close</i></span>
          <% bullets.forEach(bullet => { %>
              <p class="green-text text-darken-2"><%= bullet %></p>
          <% }); %>
        </div>
    </div>
</div> 